# ---- FreshRSS core ----
TZ=Asia/Shanghai
CRON_MIN=*/720

# ---- SiliconFlow / embeddings ----
SILICONFLOW_API_KEY=your_siliconflow_api_key_here
SILICONFLOW_BASE_URL=https://api.siliconflow.cn/v1
EMBEDDING_MODEL=Qwen/Qwen3-Embedding-8B
EMBEDDING_DIM=4096

# ---- RSS search tuning ----
CHUNK_SIZE=200
CHUNK_OVERLAP=100
SEARCH_THRESHOLD=1
CHECK_INTERVAL=36000
RETENTION_DAYS=90
LANCEDB_URI=/app/lancedb_data/freshrss
EMBEDDING_BATCH_SIZE=50
SYNC_CATEGORIES=macro
PYTHONUNBUFFERED=1

# ---- WeChat2RSS ----
LIC_EMAIL=your_email@example.com
LIC_CODE=your_license_code_here
RSS_HOST=wechat2rss:8080
RSS_PROXY_DISABLE_IMG=1
